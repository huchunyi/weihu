{
    "routes": [
      {
        "src": "/(.*)",
        "dest": "https://45.205.17.14:443/$1",
        "headers": {
          "Host": "www.bfbke.com",
          "X-Forwarded-Proto": "https",
          "X-Forwarded-For": "$remote_addr",
          "X-Real-IP": "$remote_addr",
          "X-Forwarded-Host": "www.bfbke.com",
          "X-Forwarded-Port": "443"
        }
      }
    ]
}
